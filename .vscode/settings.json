{
    "files.associations": {
        "stdio.h": "c",
        "header.h": "c",
        "encodedispatcher.h": "c",
        "aes_256_gcm.h": "c",
        "filegeneration.h": "c",
        "stat.h": "c",
        "rand.h": "c",
        "evp.h": "c",
        "entry.h": "c",
        "encryption.h": "c",
        "uu_encode.h": "c",
        "encoder.h": "c",
        "encoderdispatcher.h": "c",
        "test_encoders_uu_encode.h": "c",
        "test_entry.h": "c"
    },
    "sonarlint.pathToCompileCommands": "${workspaceFolder}/.vscode/compile_commands.json",
    "C_Cpp.errorSquiggles": "disabled",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "googletest.failed",
                "settings": {
                    "foreground": "#f00"
                }
            },
            {
                "scope": "googletest.passed",
                "settings": {
                    "foreground": "#0f0"
                }
            },
            {
                "scope": "googletest.run",
                "settings": {
                    "foreground": "#0f0"
                }
            }
        ]
    },
    "gtest-adapter.debugConfig": [
        "C/C++: g++ build and debug active file"
    ]
}